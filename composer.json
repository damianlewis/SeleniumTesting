{
    "name": "damianlewis/seleniumtesting",
    "description": "An API for phpunit-selenium based on Laravel's integrated testing framework",
    "authors": [
        {
            "name": "Damian Lewis",
            "email": "damian@damianlewis.co.uk"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.3.*",
        "phpunit/phpunit": "~5.0",
        "phpunit/phpunit-selenium": "^3.0"
    },
    "require-dev": {
        "laravel/homestead": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "SeleniumTesting\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/SeleniumTestCase.php",
            "tests/laravel/database"
        ],
        "psr-4": {
            "App\\": "tests/laravel/app/"
        }
    }
}